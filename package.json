{
  "name": "home-iot-project",
  "version": "1.0.1",
  "description": "Custom Node.js",
  "private": true,
  "engines": {
    "node": "10.15.0"
  },
  "main": "app.js",
  "scripts": {
    "build": "parcel build src/index.html",
    "start": "node --experimental-modules app.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js --experimental-modules app.js",
    "update:packages": "node wipe.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "nodemonConfig": {
    "ignore": [
      "src/*",
      "dist/*",
      ".cache/*"
    ]
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "dependencies": {
    "assert": "*",
    "babel-cli": ">=2.3.1",
    "babel-core": "*",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-preset-stage-1": "*",
    "babel-register": "*",
    "body-parser": "*",
    "cheerio": "*",
    "css-modules-require-hook": "*",
    "easyimage": "^3.1.1",
    "eslint": "^5.15.2",
    "eslint-plugin-react": "^7.12.4",
    "express": "*",
    "googleapis": "^38.0.0",
    "isomorphic-fetch": "*",
    "jsonwebtoken": "^8.5.0",
    "marked": "^0.6.1",
    "moment": "*",
    "mongoose": "^5.4.19",
    "morgan": "*",
    "node-sass": "*",
    "nodemon": "*",
    "parcel-bundler": "^1.12.2",
    "pg": "^7.9.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^3.0.0 || ^4.0.0",
    "redux-form": "^8.1.0",
    "redux-thunk": "*",
    "request": "*",
    "serialize-javascript": "*",
    "socket.io": "*",
    "style-loader": "*",
    "tesseract.js": "*"
  }
}
